{
	"extends": "airbnb",
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"rules":{
		"no-use-before-define": 0,
		"react/jsx-filename-extension": 0,
		"import/no-unresolved": 0,
		"react/prefer-stateless-function": 0,
		"indent": 0,
		"no-tabs": 0,
		"react/jsx-indent": 0,
		"react/destructuring-assignment": 0,
		"react/prop-types": 0,
		"spaced-comment": 0,
		"import/extensions": 0,
		"react/state-in-constructor": 0,
		"object-curly-newline": 0,
		"react/jsx-no-bind": 0,
		"brace-style": 0
	},
	"parser": "babel-eslint"
}